{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2022",
    "allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitOverride": true,
    "removeComments": true,
  },
}